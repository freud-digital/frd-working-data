<?xml version="1.0" encoding="UTF-8"?>
<config xmlns="http://humanistika.org/ns/tei-completer">
    <server>
        <baseUrl>https://frd-ac.acdh-dev.oeaw.ac.at</baseUrl>
    </server>
    <autoComplete>
        <context>//rs</context>
        <attribute>@ref</attribute>
        <dependent default="person">@type</dependent>
        <selection>@n</selection>
        <request>
            <url>$baseUrl/baserow/$dependent?q=$selection&amp;format=teicompleter</url>
        </request>
    </autoComplete>
